generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // Baris url dikosongkan karena sudah diatur di prisma.config.ts
}

model Todo {
  id        Int      @id @default(autoincrement())
  task      String
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
}